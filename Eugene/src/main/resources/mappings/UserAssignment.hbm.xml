<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 
 <!-- Mapping configuration details between Shipment class and shipment table -->
<hibernate-mapping package="info.pello.eugene.model">
 
    <class name="UserAssignment" table="user_assignment">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="idUser" column="iduser"/>
        <property name="idAssignment" column="idassignment" />
        <property name="since" column="since" />
        <property name="file" column="file" />
        <property name="attempts" column="attempts" />
    </class>
 
</hibernate-mapping>